[user]
	name = Aaron Bassett
	email = aaron.bassett@nantomics.com
[color]
	ui = true
[alias]
	vd = !git diff | vim -R -
	ignore = !git update-index --assume-unchanged 
	unignore = !git update-index --no-assume-unchanged
	ignored = !git ls-files -v | grep ^[a-z]
[push]
	default = tracking
[help]
	autocorrect = 1
